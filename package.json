    "@types/socket.io": "2.1.4",
    "@types/superagent": "4.1.7",
    "@types/supertest": "2.0.8",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.0",
    "apollo-datasource-rest": "0.8.0",
    "apollo-server-express": "2.14.2",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chalk": "3.0.0",
    "compression": "1.7.4",
